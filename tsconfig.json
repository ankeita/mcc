{
	"compilerOptions": {
	  "allowJs": true,
	  "allowSyntheticDefaultImports": true,
	  "esModuleInterop": true,
	  "isolatedModules": true,
	  "jsx": "react-native",	  
	  "lib": [
		"es5",
		"es6",
		"es7",
		"es2017",],
	  "moduleResolution": "node",
	  "noEmit": true,
	  "strict": true,
	  "strictNullChecks": false,
	  "target": "esnext",
	  "noImplicitAny": false
	},
	"include": [
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": [
	  "node_modules",
	  "babel.config.tsx",
	  "metro.config.js",
	  "jest.config.js"
	]
  }